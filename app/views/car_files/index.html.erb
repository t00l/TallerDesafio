<p id="notice"><%= notice %></p>

<h1>Listing Car Files</h1>

<table>
  <thead>
    <tr>
      <th>Office</th>
      <th>Car</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_files.each do |car_file| %>
      <tr>
        <td><%= car_file.office %></td>
        <td><%= car_file.car %></td>
        <td><%= link_to 'Show', car_file %></td>
        <td><%= link_to 'Edit', edit_car_file_path(car_file) %></td>
        <td><%= link_to 'Destroy', car_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car file', new_car_file_path %>
